<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yucong.lianliankan">

    <application
        android:name="org.litepal.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BaseActivity" />
        <activity android:name=".MainActivity" />



        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RankActivity" />

        <service
            android:name=".sound.MusicService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.example.yyh.lianliankan.MUSIC" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>

        <activity android:name=".view.AIDLActivity" />

        <provider
            android:name=".MyContentProvider"
            android:authorities="com.example.yyh.linaliankan"
            android:enabled="true"
            android:exported="true"></provider>
    </application>

</manifest>